#!/bin/sh

# HISTORY
#    11/14/2018 - jsb - added when adding variants to database
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

CLUSTER mgd.ALL_Variant_Effect USING ALL_Variant_Effect_pkey;

create index ALL_Variant_Effect_idx_Variant_key on mgd.ALL_Variant_Effect (_Variant_key);

create index ALL_Variant_Effect_idx_Effect_key on mgd.ALL_Variant_Effect (_Effect_key);

create index ALL_Variant_Effect_idx_CreatedBy_key on mgd.ALL_Variant_Effect (_CreatedBy_key);

create index ALL_Variant_Effect_idx_ModifiedBy_key on mgd.ALL_Variant_Effect (_ModifiedBy_key);

EOSQL
