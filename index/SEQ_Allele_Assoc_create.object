cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create index SEQ_Allele_Assoc_idx_Sequence_key USING mgd.SEQ_Allele_Assoc (_Sequence_key, _Allele_key);


create index SEQ_Allele_Assoc_idx_Allele_key USING mgd.SEQ_Allele_Assoc (_Allele_key);



CLUSTER seq_allele_assoc_pkey USING mgd.SEQ_Allele_Assoc;

EOSQL
