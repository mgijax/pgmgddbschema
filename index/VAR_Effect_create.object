#!/bin/sh

# HISTORY
#    11/14/2018 - jsb - added when adding variants to database
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

CLUSTER mgd.VAR_Effect USING VAR_Effect_pkey;

create index VAR_Effect_idx_Variant_key on mgd.VAR_Effect (_Variant_key);

create index VAR_Effect_idx_Effect_key on mgd.VAR_Effect (_Effect_key);

create index VAR_Effect_idx_CreatedBy_key on mgd.VAR_Effect (_CreatedBy_key);

create index VAR_Effect_idx_ModifiedBy_key on mgd.VAR_Effect (_ModifiedBy_key);

EOSQL
