#!/bin/sh

# 08/08/2016	jsb
#	added for TR12370 / GXD High-Throughput Indexing project

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

CLUSTER mgd.GXD_HTSampleOrganism USING GXD_HTSampleOrganism_pkey;

create index GXD_HTSampleOrganism_idx_Sample_key on mgd.GXD_HTSampleOrganism (_Sample_key);

create index GXD_HTSampleOrganism_idx_OrganismTerm_key on mgd.GXD_HTSampleOrganism (_OrganismTerm_key);

create index GXD_HTSampleOrganism_idx_CreatedBy_key on mgd.GXD_HTSampleOrganism (_CreatedBy_key);

create index GXD_HTSampleOrganism_idx_ModifiedBy_key on mgd.GXD_HTSampleOrganism (_ModifiedBy_key);

EOSQL
