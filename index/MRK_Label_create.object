cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create index MRK_Label_idx_clustered USING mgd.MRK_Label (_Marker_key, priority, _OrthologOrganism_key, labelType, label);


create index MRK_Label_idx_label USING mgd.MRK_Label (label, _Organism_key, _Marker_key);


create index MRK_Label_idx_Label_Status_key USING mgd.MRK_Label (_Label_Status_key);


create index MRK_Label_idx_Organism_key USING mgd.MRK_Label (_Organism_key);


create index MRK_Label_idx_Priority USING mgd.MRK_Label (priority);



CLUSTER mrk_label_idx_clustered USING mgd.MRK_Label;

EOSQL
