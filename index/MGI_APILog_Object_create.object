cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

CLUSTER mgd.MGI_APILog_Object USING MGI_APILog_Object_pkey;

create index MGI_APILog_Object_idx_event on mgd.MGI_APILog_Object (_Event_key);

create index MGI_APILog_Object_idx_mgitype on mgd.MGI_APILog_Object (_MGIType_key);

create index MGI_APILog_Object_idx_object on mgd.MGI_APILog_Object (_Object_key);

EOSQL
