#!/bin/sh

# HISTORY
#
# 03/31/2023    lec
# tables does not have index on purpose: _Createdby_key, _ModifiedBy_key, creation_date, modification_date
# saving some space; no need to add index if it is not used
# feel free to add these indexes, if necessary
#
# 02/24/2014    sc
#       - TR 11560; Feature Relationships
#

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

CLUSTER mgd.MGI_Relationship_Category USING MGI_Relationship_Category_pkey;

create index MGI_Relationship_Category_idx_name on mgd.MGI_Relationship_Category (name);

create index MGI_Relationship_Category_idx_CreatedBy_key on mgd.MGI_Relationship_Category (_CreatedBy_key);

create index MGI_Relationship_Category_idx_ModifiedBy_key on mgd.MGI_Relationship_Category (_ModifiedBy_key);

EOSQL
