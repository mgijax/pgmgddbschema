#!/bin/sh

# HISTORY
#    11/14/2018 - jsb - added when adding variants to database
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

CLUSTER mgd.VAR_Type USING VAR_Type_pkey;

create index VAR_Type_idx_Variant_key on mgd.VAR_Type (_Variant_key);

create index VAR_Type_idx_Type_key on mgd.VAR_Type (_Type_key);

create index VAR_Type_idx_CreatedBy_key on mgd.VAR_Type (_CreatedBy_key);

create index VAR_Type_idx_ModifiedBy_key on mgd.VAR_Type (_ModifiedBy_key);

EOSQL
