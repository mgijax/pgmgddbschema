#!/bin/sh

#
# History
#
# 03/31/2023    lec
# tables does not have index on purpose: _Createdby_key, _ModifiedBy_key, creation_date, modification_date
# saving some space; no need to add index if it is not used
# feel free to add these indexes, if necessary
#
# 10/19/2006	lec
#	- TR 6812
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

CLUSTER mgd.VOC_GO_Cache USING VOC_GO_Cache_pkey;

create index VOC_GO_Cache_idx_clustered on mgd.VOC_GO_Cache (_Term_key);

create index VOC_GO_Cache_idx_Term_key on mgd.VOC_GO_Cache (_Term_key);

create index VOC_GO_Cache_idx_DAG_key on mgd.VOC_GO_Cache (_DAG_key);

EOSQL
