#!/bin/sh

# HISTORY
#    11/14/2018 - jsb - added when adding variants to database
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

CLUSTER mgd.ALL_Variant_Type USING ALL_Variant_Type_pkey;

create index ALL_Variant_Type_idx_Variant_key on mgd.ALL_Variant_Type (_Variant_key);

create index ALL_Variant_Type_idx_Type_key on mgd.ALL_Variant_Type (_Type_key);

create index ALL_Variant_Type_idx_CreatedBy_key on mgd.ALL_Variant_Type (_CreatedBy_key);

create index ALL_Variant_Type_idx_ModifiedBy_key on mgd.ALL_Variant_Type (_ModifiedBy_key);

EOSQL
