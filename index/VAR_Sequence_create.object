#!/bin/sh

# HISTORY
#    11/14/2018 - jsb - added when adding variants to database
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

CLUSTER mgd.VAR_Sequence USING VAR_Sequence_pkey;

create index VAR_Sequence_idx_Variant_key on mgd.VAR_Sequence (_Variant_key);

create index VAR_Sequence_idx_Sequence_Type_key on mgd.VAR_Sequence (_Sequence_Type_key);

create index VAR_Sequence_idx_CreatedBy_key on mgd.VAR_Sequence (_CreatedBy_key);

create index VAR_Sequence_idx_ModifiedBy_key on mgd.VAR_Sequence (_ModifiedBy_key);

EOSQL
