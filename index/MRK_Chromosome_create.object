cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create unique index MRK_Chromosome_idx_clustered USING mgd.MRK_Chromosome (_Organism_key, chromosome);


create index MRK_Chromosome_idx_chromosome USING mgd.MRK_Chromosome (chromosome);



CLUSTER mrk_chromosome_idx_clustered USING mgd.MRK_Chromosome;

EOSQL
