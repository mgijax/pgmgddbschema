#!/bin/sh

# HISTORY
#    11/14/2018 - jsb - added when adding variants to database
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

CLUSTER mgd.VAR_Variant USING VAR_Variant_pkey;

create index VAR_Variant_idx_Allele_key on mgd.VAR_Variant (_Allele_key);

create index VAR_Variant_idx_SourceVariant_key on mgd.VAR_Variant (_SourceVariant_key);

create index VAR_Variant_idx_Strain_key on mgd.VAR_Variant (_Strain_key);

create index VAR_Variant_idx_CreatedBy_key on mgd.VAR_Variant (_CreatedBy_key);

create index VAR_Variant_idx_ModifiedBy_key on mgd.VAR_Variant (_ModifiedBy_key);

EOSQL
