#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.VAR_Effect IS 'A record in this table represents one effect of a variant.';

COMMENT ON COLUMN VAR_Effect._VariantEffect_key IS 'primary key, uniquely identifying the record';

COMMENT ON COLUMN VAR_Effect._Variant_key IS 'foreign key to VAR_Variant, identifying the variant with this effect';

COMMENT ON COLUMN VAR_Effect._Effect_key IS 'foreign key to VOC_Term, identifies an effect of this variant';

COMMENT ON COLUMN VAR_Effect._CreatedBy_key IS 'user who created the record';

COMMENT ON COLUMN VAR_Effect.creation_date IS 'date record was created';

COMMENT ON COLUMN VAR_Effect.modification_date IS 'date record was last modified';

COMMENT ON COLUMN VAR_Effect._ModifiedBy_key IS 'user who last modified the record';

EOSQL