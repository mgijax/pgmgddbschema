#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.MGI_EMAPS_Mapping IS 'A record in this table represents a mapping between an accession ID, e,g, Anatomical Dictionary term ID, and an EMAPS term ID. 

See EMAPS vocab: VOC_Term._Vocab_key = 91';

COMMENT ON COLUMN MGI_EMAPS_Mapping.accID IS 'Accession ID mapped to ''emapsID''';

COMMENT ON COLUMN MGI_EMAPS_Mapping._CreatedBy_key IS 'user who created the record';

COMMENT ON COLUMN MGI_EMAPS_Mapping.creation_date IS 'date record was created';

COMMENT ON COLUMN MGI_EMAPS_Mapping.emapsID IS 'EMAPS ID mapped to ''accID''';

COMMENT ON COLUMN MGI_EMAPS_Mapping._Mapping_key IS 'Primary key';

COMMENT ON COLUMN MGI_EMAPS_Mapping.modification_date IS 'date record was last modified';

COMMENT ON COLUMN MGI_EMAPS_Mapping._ModifiedBy_key IS 'user who last modified the record';

EOSQL