#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.NOM_Marker IS 'A record in this table represents a specific Nomen object.';

COMMENT ON COLUMN NOM_Marker._BroadcastBy_key IS 'user who broadcast the record (foreign key to MGI_User)';

COMMENT ON COLUMN NOM_Marker.broadcast_date IS 'date record was broadcast to MGI';

COMMENT ON COLUMN NOM_Marker.chromosome IS 'chromosome';

COMMENT ON COLUMN NOM_Marker._CreatedBy_key IS 'user who created the record';

COMMENT ON COLUMN NOM_Marker.creation_date IS 'date record was created';

COMMENT ON COLUMN NOM_Marker._CurationState_key IS 'foreign key to VOC_Term where _Vocab_key = 15 (Curation State)';

COMMENT ON COLUMN NOM_Marker.humanSymbol IS 'human ortholog';

COMMENT ON COLUMN NOM_Marker._Marker_Event_key IS 'foreign key to MRK_Event';

COMMENT ON COLUMN NOM_Marker._Marker_EventReason_key IS 'foreign key to MRK_EventReason';

COMMENT ON COLUMN NOM_Marker._Marker_Type_key IS 'foreign key to MRK_Types';

COMMENT ON COLUMN NOM_Marker.modification_date IS 'date record was last modified';

COMMENT ON COLUMN NOM_Marker._ModifiedBy_key IS 'user who last modified the record';

COMMENT ON COLUMN NOM_Marker.name IS 'name';

COMMENT ON COLUMN NOM_Marker._Nomen_key IS 'primary key';

COMMENT ON COLUMN NOM_Marker._NomenStatus_key IS 'foreign key to VOC_Term';

COMMENT ON COLUMN NOM_Marker.statusNote IS 'status notes';

COMMENT ON COLUMN NOM_Marker.symbol IS 'symbol';

EOSQL