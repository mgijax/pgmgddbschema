#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.GXD_HTSample_RNASeqCombined IS 'A record in this table represents an experiment/sample/gene combined biological replicates';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined._RNASeqCombined_key IS 'primary key';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined._Marker_key IS 'foreign key to MRK_Marker';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined._Level_key IS 'foreign key to VOC_Term where _Vocab_key = ?? (RNA-Seq TPM Level)';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined.numberOfBiologicalReplicates IS 'the number of biological replicates';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined.averageQuantileNormalizedTPM IS 'the average quantile normlized TPM';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined.note IS '';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined.age IS 'textual age';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined._Organism_key IS 'foreign key to MGI_Organism, identifying type of organism';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined._Sex_key IS 'foreign key to VOC_Term, identifying sex';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined._Emapa_key IS 'foreign key to VOC_Term, identifying the EMAPA structure';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined._Stage_key IS 'foreign key to GXD_TheilerStage, identifying the Theiler Stage';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined._Genotype_key IS 'foreign key to GXD_Genotype, identifying the genotype';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined._CreatedBy_key IS 'user who created the record';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined.creation_date IS 'date record was created';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined.modification_date IS 'date record was last modified';

COMMENT ON COLUMN GXD_HTSample_RNASeqCombined._ModifiedBy_key IS 'user who last modified the record';

EOSQL
