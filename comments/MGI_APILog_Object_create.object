#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.MGI_APILog_Object IS 'A record in this table represents one call to an API endpoint that alters (create / update / delete) data.';

COMMENT ON COLUMN MGI_APILog_Object._LogObject_key IS 'primary key for this table';

COMMENT ON COLUMN MGI_APILog_Object._Event_key IS 'foreign key to MGI_APILog_Event table';

COMMENT ON COLUMN MGI_APILog_Object._MGIType_key IS 'foreign key to ACC_MGIType table';

COMMENT ON COLUMN MGI_APILog_Object._Object_key IS 'specifies object to which a change was made because of the call to the API endpoint';

EOSQL