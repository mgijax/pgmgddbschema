#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.VOC_Allele_Cache IS 'A record in this table represents the cached value of the association between an Allele and an Annotation Type.  The Annotation Types are VOC_AnnotType: 1002:Mammalian Phentype/Genotype, 1020:DO/Genotype. The cache is loaded from mgicacheload/vocallele.csh which calls stored procedures in pgmgddbschema/procedure (VOC_Allele_Cache, VOC_GO_Cache, VOC_Marker_Cache).';

COMMENT ON COLUMN VOC_Allele_Cache._Allele_key IS 'foreign key to ALL_Allele';

COMMENT ON COLUMN VOC_Allele_Cache.annotType IS 'see table description for details';

COMMENT ON COLUMN VOC_Allele_Cache._Term_key IS 'foreign key to VOC_Term';

EOSQL
