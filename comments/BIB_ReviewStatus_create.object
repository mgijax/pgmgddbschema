#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.BIB_ReviewStatus IS 'A record in this table represents a Review Status vocabulary term.';

COMMENT ON COLUMN BIB_ReviewStatus.creation_date IS 'date record was created';

COMMENT ON COLUMN BIB_ReviewStatus.modification_date IS 'date record was last modified';

COMMENT ON COLUMN BIB_ReviewStatus.name IS 'status description';

COMMENT ON COLUMN BIB_ReviewStatus._ReviewStatus_key IS 'primary key';

EOSQL