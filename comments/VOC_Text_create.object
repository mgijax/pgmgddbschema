#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.VOC_Text IS 'A record in this table represents textual information of a Vocabulary Term';

COMMENT ON COLUMN VOC_Text.creation_date IS 'date record was created';

COMMENT ON COLUMN VOC_Text.modification_date IS 'date record was last modified';

COMMENT ON COLUMN VOC_Text.note IS 'textual information';

COMMENT ON COLUMN VOC_Text._Term_key IS 'foreign key to VOC_Term';

EOSQL
