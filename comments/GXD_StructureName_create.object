#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.GXD_StructureName IS 'Official and Synonym table for Anatomical Structures.  If the GXD_StructureName._StructureName_key equals GXD_Structure._StructureName_key, then this is the *official* name of the symbol.  All other "names" are synonyms.  See GXD_Structure.';

COMMENT ON COLUMN GXD_StructureName.creation_date IS 'date record was created';

COMMENT ON COLUMN GXD_StructureName.mgiAdded IS 'has this name been added by the MGI group?  yes (1), no (0)';

COMMENT ON COLUMN GXD_StructureName.modification_date IS 'date record was last modified';

COMMENT ON COLUMN GXD_StructureName.structure IS 'description';

COMMENT ON COLUMN GXD_StructureName._Structure_key IS 'foreign key to GXD_Structure';

COMMENT ON COLUMN GXD_StructureName._StructureName_key IS 'primary key';

EOSQL