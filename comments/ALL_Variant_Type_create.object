#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.ALL_Variant_Type IS 'A record in this table represents a sequence associated with a variant.n have multiple variants. Since each variant will have two records in ALL_Variant (one a source record, the other a curated record), there will be sequence records associated with both types of variant records.';

COMMENT ON COLUMN ALL_Variant_Type._VariantEffect_key IS 'primary key, uniquely identifying the record';

COMMENT ON COLUMN ALL_Variant_Type._Variant_key IS 'foreign key to ALL_Variant, identifying the variant with this effect';

COMMENT ON COLUMN ALL_Variant_Type._Type_key IS 'foreign key to VOC_Term, identifies a type of this variant';

COMMENT ON COLUMN ALL_Variant_Type._CreatedBy_key IS 'user who created the record';

COMMENT ON COLUMN ALL_Variant_Type.creation_date IS 'date record was created';

COMMENT ON COLUMN ALL_Variant_Type.modification_date IS 'date record was last modified';

COMMENT ON COLUMN ALL_Variant_Type._ModifiedBy_key IS 'user who last modified the record';

EOSQL
