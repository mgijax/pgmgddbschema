#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.MRK_OMIM_Cache IS 'A record in this table represents the cached value of a mouse genotype/allele/OMIM annotation or a human marker/OMIM annotation. See mrkcacheload/mrkomim.csh.';

COMMENT ON COLUMN MRK_OMIM_Cache._Allele_key IS 'foreign key to ALL_Allele';

COMMENT ON COLUMN MRK_OMIM_Cache.alleleSymbol IS 'allele symbol (mouse only)';

COMMENT ON COLUMN MRK_OMIM_Cache._Cache_key IS 'primary key';

COMMENT ON COLUMN MRK_OMIM_Cache.creation_date IS 'date record was created';

COMMENT ON COLUMN MRK_OMIM_Cache.genotypeDisplay1 IS 'genotype display text for WI';

COMMENT ON COLUMN MRK_OMIM_Cache.genotypeDisplay2 IS 'genotype display text for WI';

COMMENT ON COLUMN MRK_OMIM_Cache.genotypeFootnote IS 'genotype footnote for WI';

COMMENT ON COLUMN MRK_OMIM_Cache._Genotype_key IS 'foreign key to GXD_Genotype';

COMMENT ON COLUMN MRK_OMIM_Cache.header IS 'genotype header for WI';

COMMENT ON COLUMN MRK_OMIM_Cache.headerFootnote IS 'genotype header footnote for WI';

COMMENT ON COLUMN MRK_OMIM_Cache.jnumID IS 'J: id of Annotation reference';

COMMENT ON COLUMN MRK_OMIM_Cache._Marker_key IS 'foreign key to MRK_Marker';

COMMENT ON COLUMN MRK_OMIM_Cache.markerSymbol IS 'marker symbol (mouse only)';

COMMENT ON COLUMN MRK_OMIM_Cache._Marker_Type_key IS 'foreign key to MRK_Types';

COMMENT ON COLUMN MRK_OMIM_Cache.modification_date IS 'date record was last modified';

COMMENT ON COLUMN MRK_OMIM_Cache.omimCategory1 IS 'display category for Human Disease Detail page, table 1';

COMMENT ON COLUMN MRK_OMIM_Cache.omimCategory2 IS 'display category for Human Disease Detail page, table 2';

COMMENT ON COLUMN MRK_OMIM_Cache.omimCategory3 IS 'display category for Allele Detail page';

COMMENT ON COLUMN MRK_OMIM_Cache._Organism_key IS 'foreign key to MGI_Organism';

COMMENT ON COLUMN MRK_OMIM_Cache._OrthologMarker_key IS 'foreign key to MRK_Marker; human ortholog if _Organism_key = 1';

COMMENT ON COLUMN MRK_OMIM_Cache._OrthologOrganism_key IS 'foreign key to MGI_Organism; human organism key if _Organism_key = 1';

COMMENT ON COLUMN MRK_OMIM_Cache.orthologSymbol IS 'mouse ortholog symbol';

COMMENT ON COLUMN MRK_OMIM_Cache.qualifier IS 'OMIM qualifier';

COMMENT ON COLUMN MRK_OMIM_Cache._Refs_key IS 'foreign key to BIB_Refs; reference for OMIM annotation';

COMMENT ON COLUMN MRK_OMIM_Cache.sequenceNum IS 'genotype order';

COMMENT ON COLUMN MRK_OMIM_Cache.strain IS 'strain of genotype';

COMMENT ON COLUMN MRK_OMIM_Cache.term IS 'OMIM term';

COMMENT ON COLUMN MRK_OMIM_Cache.termID IS 'OMIM term ID';

COMMENT ON COLUMN MRK_OMIM_Cache._Term_key IS 'foreign key to VOC_Term where _Vocab_key = 44 (OMIM)';

EOSQL