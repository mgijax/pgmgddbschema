#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.MGI_Tables IS 'A record in this table represents the comments for a table.';

COMMENT ON COLUMN MGI_Tables.creation_date IS 'date record was created';

COMMENT ON COLUMN MGI_Tables.description IS 'description of the table';

COMMENT ON COLUMN MGI_Tables._LoadedBy_key IS 'user who last loaded this information';

COMMENT ON COLUMN MGI_Tables.loaded_date IS 'date record was last loaded';

COMMENT ON COLUMN MGI_Tables.modification_date IS 'date record was last modified';

COMMENT ON COLUMN MGI_Tables.table_name IS 'table name';

EOSQL