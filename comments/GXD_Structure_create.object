#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.GXD_Structure IS 'Master table of Anatomical Dictionary.

See GXD_StructureClosure, GXD_StructureName, GXD_TheilerStage.';

COMMENT ON COLUMN GXD_Structure.creation_date IS 'date record was created';

COMMENT ON COLUMN GXD_Structure.edinburghKey IS 'Edinburgh Accession ID';

COMMENT ON COLUMN GXD_Structure.inheritSystem IS 'if 1 (true), then structure inherits its system from its parent, else 0 (this is the parent)';

COMMENT ON COLUMN GXD_Structure.modification_date IS 'date record was last modified';

COMMENT ON COLUMN GXD_Structure._Parent_key IS 'foreign key to GXD_Structure; parent structure';

COMMENT ON COLUMN GXD_Structure.printName IS 'always the same as GXD_StructureName (primary name)';

COMMENT ON COLUMN GXD_Structure.printStop IS 'stop here when deriving the printName?
obsolete : no longer used';

COMMENT ON COLUMN GXD_Structure._Stage_key IS 'foreign key to GXD_TheilerStage';

COMMENT ON COLUMN GXD_Structure._Structure_key IS 'primary key';

COMMENT ON COLUMN GXD_Structure._StructureName_key IS 'foreign key to GXD_StructureName; primary name';

COMMENT ON COLUMN GXD_Structure.structureNote IS 'textual information';

COMMENT ON COLUMN GXD_Structure._System_key IS 'foreign key to VOC_Term where _Vocab_key = 75 (Anatomical System)';

COMMENT ON COLUMN GXD_Structure.topoSort IS 'topological sort key';

COMMENT ON COLUMN GXD_Structure.treeDepth IS 'depth of structure within the tree';

EOSQL