#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.BIB_DataSet IS 'A record in this table represents a MGI DataSet.  Each DataSet corresonds to a curatorial area within MGI and enables us to index References to particular curatorial areas.  Examples are Molecular Segments, Mapping, Gene Ontology.';

COMMENT ON COLUMN BIB_DataSet.abbreviation IS 'data set abbreviation';

COMMENT ON COLUMN BIB_DataSet._CreatedBy_key IS 'user who created the record';

COMMENT ON COLUMN BIB_DataSet.creation_date IS 'date record was created';

COMMENT ON COLUMN BIB_DataSet.dataSet IS 'data set name';

COMMENT ON COLUMN BIB_DataSet._DataSet_key IS 'primary key';

COMMENT ON COLUMN BIB_DataSet.inMGIprocedure IS 'stored procedure to check if given Reference is in this particular data set';

COMMENT ON COLUMN BIB_DataSet.isObsolete IS 'if 1, then data set is obsolete';

COMMENT ON COLUMN BIB_DataSet.modification_date IS 'date record was last modified';

COMMENT ON COLUMN BIB_DataSet._ModifiedBy_key IS 'user who last modified the record';

COMMENT ON COLUMN BIB_DataSet.sequenceNum IS 'sequence number for ordering records';

EOSQL
