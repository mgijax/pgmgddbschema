#!/bin/sh

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

COMMENT ON TABLE mgd.GXD_HTSampleOrganism IS 'A record in this table represents one organism associated with a sample (GXD_HTSample) for a high-throughput expression experiment (GXD_HTExpression).  We do not use the standard MGI_Organism table, as these data also include crosses, various mouse strains, etc.';

COMMENT ON COLUMN GXD_HTSampleOrganism._SampleOrganism_key IS 'primary key';

COMMENT ON COLUMN GXD_HTSampleOrganism._Sample_key IS 'foreign key to GXD_HTSample, identifies the sample with which this organism is associated';

COMMENT ON COLUMN GXD_HTSampleOrganism._OrganismTerm_key IS 'foreign key to VOC_Term, identifies the type of organism';

COMMENT ON COLUMN GXD_HTSampleOrganism._CreatedBy_key IS 'foreign key to MGI_User; user who created the record';

COMMENT ON COLUMN GXD_HTSampleOrganism.creation_date IS 'date record was created';

COMMENT ON COLUMN GXD_HTSampleOrganism._ModifiedBy_key IS 'foreign key to MGI_User; user who last modified the record';

COMMENT ON COLUMN GXD_HTSampleOrganism.modification_date IS 'date record was last modified';

EOSQL
