#!/bin/sh

#
#

cd `dirname $0` && . ./Configuration

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

CREATE OR REPLACE FUNCTION PG-TABLE_update() RETURNS TRIGGER AS \$\$
BEGIN

RETURN NEW;

END;
\$\$
LANGUAGE plpgsql;

GRANT EXECUTE ON FUNCTION PG-TABLE_update() TO public;

DROP TRIGGER PG-TABLE_update_trigger ON PG-TABLE;

CREATE TRIGGER PG-TABLE_update_trigger
AFTER DELETE on PG-TABLE
FOR EACH ROW
EXECUTE PROCEDURE PG-TABLE_update();

EOSQL
