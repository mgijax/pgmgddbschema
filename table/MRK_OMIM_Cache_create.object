 	_Genotype_key                  int             null,
 	_Term_key                      int             not null,
 	_Refs_key                      int             not null,
	_OrthologOrganism_key          int             null,
	_OrthologMarker_key            int             null,
	omimCategory1                  int             not null,
	omimCategory2                  int             not null,
	omimCategory3                  int             not null,
	sequenceNum                    int             null,
	qualifier                      varchar(50)     null,
	markerSymbol                   varchar(50)     not null,
	term                           text    not null,
	termID                         varchar(30)     not null,
	jnumID                         varchar(30)     not null,
	alleleSymbol                   varchar(60)     null,
	orthologSymbol                 varchar(50)     null,
	strain                         text    null,
	genotypeDisplay1               text    null,
	genotypeDisplay2               varchar(100)    null,
	header                         varchar(150)    null,
	headerFootnote                 varchar(200)    null,
	genotypeFootnote               text    null,
	creation_date                  timestamp DEFAULT now()        not null,
	modification_date              timestamp DEFAULT now()        not null 
);
EOSQL
