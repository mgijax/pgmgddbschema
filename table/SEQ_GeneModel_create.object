#!/bin/sh

# HISTORY
#
# 02/18/2010	lec
#	- TR 9239; changed _Marker_Type_key to _GMMarker_Type_key per Sharon
#	  to differenciate the "main" marker type key from the
#	  marker types generated by the gene model biotype load
#	  added created by/date fields
#
#     10/06/2008 - jsb - added for search tool release, TR9305
#
# cache of which markers are associated with with vocabulary terms
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create table mgd.SEQ_GeneModel (
 	_Sequence_key		int		not null,
	_GMMarker_Type_key	int		null,
	rawBiotype		varchar(255)	null,
	exonCount		int		null,
	transcriptCount		int		null,
        _CreatedBy_key                 int DEFAULT 1001             not null,
        _ModifiedBy_key                int DEFAULT 1001             not null,
        creation_date              timestamp DEFAULT now()        not null,
        modification_date          timestamp DEFAULT now()        not null 
);
EOSQL
