#!/bin/sh

#
# History
#	02/16/2015 - kstone - P-WI Specific Table
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create table mgd.PWI_Report (
	id			numeric			identity,
	name			varchar(60)		not null,
	description		text			null,
	sql_text		text			not null,
	report_author		varchar(30)		not null,
	requested_by		varchar(60)		not null,
	created			timestamp DEFAULT now()		not null,
	last_run		timestamp DEFAULT now()		null,
	last_run_duration	varchar(20)			null,
	report_views		varchar(20)			null,
	type			varchar(20)			null
);
EOSQL
