cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create table mgd.NOM_Marker (
 	_Nomen_key                     int             not null,
	_Marker_Type_key               int             not null,
	_NomenStatus_key               int             not null,
	_Marker_Event_key              int             not null, -- always 1 (assigned)
	_Marker_EventReason_key        int             not null, -- always -1 (not specified)
	_CurationState_key             int             not null, -- always 166894 (internal)
	symbol                         varchar(50)     not null,
	name                           text    not null,
	chromosome                     varchar(8)      not null,
	humanSymbol                    varchar(50)     null, -- no longer used
	statusNote                     text    null,
	broadcast_date                 timestamp DEFAULT now()        null,
	_BroadcastBy_key               int             null,
	_CreatedBy_key                 int DEFAULT 1001             not null,
	_ModifiedBy_key                int DEFAULT 1001             not null,
	creation_date                  timestamp DEFAULT now()        not null,
	modification_date              timestamp DEFAULT now()        not null 
);
EOSQL
