cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create view mgd.MRK_Types_Summary_View
as
select a._Accession_key,
       a.accID, 
       a.prefixPart, 
       a.numericPart, 
       a._LogicalDB_key, 
       t._Marker_Type_key as _Object_key,
       a._MGIType_key, 
       a.private, 
       a.preferred, 
       a.creation_date, 
       a.modification_date,
       t.name as mgiID,
       t.name as subType,
       t.name as description,
       t.name as short_description
from MRK_Types t
	LEFT OUTER JOIN ACC_Accession a on (t._Marker_Type_key = a._Object_key and a._MGIType_key = 37)




;

EOSQL
