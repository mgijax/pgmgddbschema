cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create view mgd.MGI_AttrHistory_Source_View
as
select a.*, u1.login as createdBy, u2.login as modifiedBy
from MGI_AttributeHistory a, MGI_User u1, MGI_User u2
where a._MGIType_key = 5
and a._CreatedBy_key = u1._User_key
and a._ModifiedBy_key = u2._User_key



;

EOSQL
