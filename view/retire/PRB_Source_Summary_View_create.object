cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create view mgd.PRB_Source_Summary_View
as
select a._Accession_key, 
       a.accID, 
       a.prefixPart,
       a.numericPart,
       a._LogicalDB_key, 
       s._Source_key as _Object_key, 
       a._MGIType_key,
       a.private,
       a.preferred,
       a.creation_date,
       a.modification_date,
       a2.accID as mgiID,
       a2.accID as subType,
       s.name as description,
       s.name as short_description
from PRB_Source s
	LEFT OUTER JOIN ACC_Accession a on (s._Source_key = a._Object_key and a._MGIType_key = 5)
        LEFT OUTER JOIN ACC_Accession a2 on (a._Object_key = a2._Object_key
		and a2._LogicalDB_key = 1
		and a2._MGIType_key = 5
		and a2.prefixPart = 'MGI:'
		and a2.preferred = 1)
where s.name is not null



;

EOSQL
