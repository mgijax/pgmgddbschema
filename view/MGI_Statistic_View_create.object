#!/bin/csh -f

# History:
# 	10/23/07 - jsb - added for Home Page release (MGI 4.0), TR 8087
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create view mgd.MGI_Statistic_View
as
select mset.name as groupName,
	msm.sequenceNum,
	ms._Statistic_key,
	ms.name,
	ms.abbreviation,
	ms.definition,
	ms.isPrivate,
	ms.hasIntValue,
	mm.timeRecorded,
	mm.intValue,
	mm.floatValue
from MGI_Measurement mm,
	MGI_Statistic ms,
	MGI_SetMember msm,
	MGI_Set mset
where mm.isLatest = 1
	and mm._Statistic_key = ms._Statistic_key
	and ms._Statistic_key = msm._Object_key
	and msm._Set_key = mset._Set_key
	and mset._MGIType_key = (select _MGIType_key
		from ACC_MGIType
		where name = 'Statistic')


;

EOSQL
