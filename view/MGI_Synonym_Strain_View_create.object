cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create view mgd.MGI_Synonym_Strain_View
as
select s.*, t._Organism_key, t.synonymType, t.allowOnlyOne, u1.login as modifiedBy
from MGI_Synonym s, MGI_SynonymType t, MGI_User u1
where s._SynonymType_key = t._SynonymType_key
and t._MGIType_key = 10
and s._ModifiedBy_key = u1._User_key



;

EOSQL
