cd `dirname $0` && . ./Configuration

${PG_MGD_DBSCHEMADIR}/view/VOC_Term_NomenStatus_View_drop.object

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create view mgd.VOC_Term_NomenStatus_View
as
select v.name, t.*
from VOC_Vocab v, VOC_Term t
where v.name = 'Nomen Status'
and v._Vocab_key = t._Vocab_key



;

EOSQL
