cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create view mgd.VOC_Term_CellLine_View
as
select v.name, t.*
from VOC_Vocab v, VOC_Term t
where v.name = 'Cell Line'
and v._Vocab_key = t._Vocab_key



;

EOSQL
