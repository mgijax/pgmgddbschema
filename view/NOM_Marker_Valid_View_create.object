cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create view mgd.NOM_Marker_Valid_View
as
/* nomen markers with status which are okay to use in Alleles */
select n.*
from NOM_Marker n, VOC_Term s
where n._NomenStatus_key = s._Term_key
and s.term in ('In Progress', 'Reserved')



;

EOSQL
