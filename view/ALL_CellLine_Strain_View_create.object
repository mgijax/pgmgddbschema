#!/bin/sh

# HISTORY
#   10/02/2008 - jsb - added for gene trap LF (TR7493) release
#

cd `dirname $0` && . ./Configuration


cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create view mgd.ALL_CellLine_Strain_View
as
select a.*,
    case
	when a.isMutant = 0 then 'Strain of Specimen'
	when a.isMutant = 1 then 'Strain of Origin'
    end as strainAssocType
from ALL_CellLine a


;

EOSQL
