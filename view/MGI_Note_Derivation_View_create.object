cd `dirname $0` && . ./Configuration

${PG_MGD_DBSCHEMADIR}/view/MGI_Note_Derivation_View_drop.object

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create view mgd.MGI_Note_Derivation_View
as
select n.*, t.noteType, c.note, c.sequenceNum
from MGI_Note n, MGI_NoteType t, MGI_NoteChunk c
where n._NoteType_key = t._NoteType_key
and t._MGIType_key = 36
and n._Note_key = c._Note_key



;

EOSQL
