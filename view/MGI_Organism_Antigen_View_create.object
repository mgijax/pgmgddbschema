cd `dirname $0` && . ./Configuration

${PG_MGD_DBSCHEMADIR}/view/MGI_Organism_Antigen_View_drop.object

cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create view mgd.MGI_Organism_Antigen_View
as 
select s.*
from MGI_Organism s, MGI_Organism_MGIType t
where s._Organism_key = t._Organism_key
and t._MGIType_key = 7


;

EOSQL
