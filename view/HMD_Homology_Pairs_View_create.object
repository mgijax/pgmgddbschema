create view mgd.HMD_Homology_Pairs_View
as
/* view of all homology pairs in the database */
select distinct h1._Class_key, h1._Homology_key,
hm1._Marker_key as markerkey1,  
m1._Organism_key as organismkey1, 
m1.symbol as marker1, 
m2.symbol as marker2, 
m2._Organism_key as organismkey2, 
hm2._Marker_key as markerkey2
from HMD_Homology h1, HMD_Homology_Marker hm1,  MRK_Marker m1,
HMD_Homology h2, HMD_Homology_Marker hm2, MRK_Marker m2
where h1._Homology_key = hm1._Homology_key
and hm1._Marker_key = m1._Marker_key
and h1._Class_key = h2._Class_key
and h2._Homology_key = hm2._Homology_key
and hm2._Marker_key = m2._Marker_key
and m1._Organism_key != m2._Organism_key


;

