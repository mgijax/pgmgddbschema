cat - <<EOSQL | ${PG_DBUTILS}/bin/doisql.csh $0

create view mgd.MGI_Organism_Acc_View
as
select a.*, l.name as LogicalDB
from ACC_Accession a, ACC_LogicalDB l
where a._MGIType_key = 20
and a._LogicalDB_key = l._LogicalDB_key



;

