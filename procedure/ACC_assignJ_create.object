CREATE OR REPLACE FUNCTION ACC_assignJ (
objectKey int,
nextMGI int = -1
)
RETURNS VOID AS
\$\$

DECLARE
j_exists int;

BEGIN

IF nextMGI != -1
THEN
	j_exists := count(*) FROM BIB_Acc_View
